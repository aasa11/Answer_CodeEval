#!/usr/bin/python

import sys

def do_array(ss):
	N, ss_l = ss.split(';')
	N = int(N)
	sum_lst = sum(int(x) for x in ss_l.split(','))
	sum_u = (1+N-2)*(N-2)/2
	print sum_lst-sum_u

def array_absurdity(infile):
	f = open(infile, 'r')
	for lines in f.readlines():
		do_array(lines)
	f.close()
	
array_absurdity(sys.argv[1])

